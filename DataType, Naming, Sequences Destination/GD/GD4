#include <stdio.h>
#include <stdlib.h>
#include <stdbool.h>

int main(int argc, char *argv[]) {
	int x = 3 , y = 5;
	bool z;
	
	printf("1 : Benar/True\n");
	printf("0 : Salah/False\n\n");
	
	z=3>5;
	printf("3 lebih dari 5? %d\n", z);
	
	z=3<5;
	printf("3 kurang dari 5? %d\n", z);
	
	z=3>=5;
	printf("3 lebih dari sama dengan 5? %d\n", z);
	
	z=3<=5;
	printf("3 kurang dari sama dengan 5? %d\n", z);
	
	z=3==5;
	printf("3 sama dengan 5? %d\n", z);
	
	z=3!=5;
	printf("3 tidak sama dengan 5? %d\n", z);
	
	system("pause");
	return 0;
}